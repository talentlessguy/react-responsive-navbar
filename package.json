    "@types/react": "^16.9.17",
    "microbundle": "^0.11.0",
    "typescript": "^3.7.5"
  },
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/talentlessguy/react-responsive-navbar/issues"
